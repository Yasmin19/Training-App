<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.devpair5.trainingapp2.MainActivity">


    <include layout="@layout/fragment_bottom_tab_bar"
        android:id="@+id/bottom_tab_bar"
        app:layout_constraintBottom_toBottomOf="parent" />


    <fragment
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        tools:context="com.example.devpair5.trainingapp2.MapsActivity"
        app:layout_constraintBottom_toTopOf="@+id/bottom_tab_bar"
        app:layout_constraintLeft_toLeftOf="parent"/>

